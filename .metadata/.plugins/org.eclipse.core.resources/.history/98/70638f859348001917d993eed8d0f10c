package Array;

public class FrequencyOfElements {

	public static void main(String[] args) 
	{
		
		int a[]={1,2,4,5,2,2,4,4,5,4};
		
		int fr[]=new int[a.length];
		
		int visited=-1;
		for(int i=0;i<a.length;i++)
		{  
			int c=1;
			for(int j=i+1; j< a.length;j++)
			{
				
				if(a[i]==a[j])
				{
					c++;
					fr[j]=visited;
				}
			}
			
		fr[i]=c;
		}
		
		
	for(int i=0;i<a.length;i++)
	{
		if(fr[i]!=visited)
		{
		System.out.println(a[i]+"-----"+fr[i]);	
		}
	}
		

	}

}
