package Array;

import java.util.Arrays;

public class RemoveDup {

	public static void main(String[] args) 
	{
		int a[]={2,1,3,1,2,4,5,3};
		Arrays.sort(a);
		
		

	}
	
	
	public static int removedeplicate(int a[], int n)
	{
		
		if(n==1)
		{
			return n;
			
		}
		
		int temp[]=new int[a.length];
		
		int j=0;
		for(int i=0; i<a.length;i++)
		{
				if(a[i]!=a[i+1])
				{
					temp[j++]=a[i];
				}			
			
		}		
		
		temp[j]=a[n-1];
		for(int i=0; i<j;i++)
		{
			a[i]=temp[i];
			
		}	
		
		
		return j;
		
		
		
		
		
	}

}
